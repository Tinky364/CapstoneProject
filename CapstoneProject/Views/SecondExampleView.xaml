<UserControl
    x:Class="CapstoneProject.Views.SecondExampleView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:CapstoneProject.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <StackPanel>
        <TextBox Text="{Binding Key, UpdateSourceTrigger=PropertyChanged}" />
        <Button
            Margin="10,0,0,0"
            Command="{Binding SubmitCommand}"
            Content="Submit" />
        <Button
            Margin="10,0,0,0"
            Command="{Binding GoToFirstExampleCommand}"
            Content="Go to First Example" />
    </StackPanel>
</UserControl>
